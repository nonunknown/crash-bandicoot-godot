script/source = "extends AcceptDialog

enum {GET,PATCH}

var reg:RegEx
var request_type = GET
var making_request:bool = false
var done:bool = false
var database
func _ready():
	reg = RegEx.new()
	var f = File.new()
	f.open(\"res://database.json\",File.READ)
	var json = JSON.parse(f.get_as_text())
	database = json.result
	f.close()
	LevelManager.connect(\"can_start_scene\",self,\"_on_can_start_scene\")

func is_valid() -> Dictionary:
	var nick = $VBoxContainer/LineEdit.text
	var spaces = nick.find(\" \")
	if spaces > 0:
		return {valid=false,reason=\"Contain Spaces\"}
	if nick.length() < 4 or nick.length() > 15:
		return {valid=false,reason=\"Length must be 4-15\"}
	
	reg.compile(\"^[a-zA-Z0-9_]*$\")
	var result = reg.search(nick)
	if result:
		return {valid=true,reason=\"Ok this nick is cool, wait a minute, Im looking if it already exists....\"}
	else:
		return {valid=false, reason=\"Error Doesnt match pattern! [a-b,A-Z,0-9]\"}
	
	
	return {valid=true,reason=\"\"}

var _nick
func _on_AcceptDialog_confirmed():
	if done:
		hide()
	if making_request: return
	var result = is_valid()
	$VBoxContainer/Error.text = result.reason
	if result.valid == false: return
	request_type = GET
	_nick = $VBoxContainer/LineEdit.text
	$HTTPRequest.request(database[\"user_url\"] % _nick)
	making_request = true
	
	pass # Replace with function body.


func _on_request_completed(result, response_code, headers, body):
	if response_code != 200:
		$VBoxContainer/Error.text = \"ops something went wrong: ERROR - %s\" % [response_code]
		making_request = false
		return
	if request_type == GET: _get_request(response_code,body)
	else: _patch_request(response_code,body)
		
	
	
	pass # Replace with function body.

func _patch_request(response_code,body):
	var json = JSON.parse(body.get_string_from_utf8())
	for key in json.result:
		$VBoxContainer/Error.text = \"You are Registered, Click ok again and wait for game to load!\"
		LevelManager.UID = key
		done = true
		print(\"key %s\" % key)
		return
	pass

func _get_request(response_code,body):
	var json = JSON.parse(body.get_string_from_utf8())
	
	if json.result == null:
		$VBoxContainer/Error.text = \"Nice This name is not being used, Wait I'm inserting it on the database...\"
		request_type = PATCH
		var data = '{\"%s\":900.10}' % _nick
		print(data)
		$HTTPRequest.request(database[\"url\"],[],false,HTTPClient.METHOD_PATCH,data)
		
		
		#TODO: Goto game
		return
	else:
		$VBoxContainer/Error.text = \"This nick is already in use, Try another :/\"
		making_request = false
		return


func _on_bt_offline_pressed():
	LevelManager.set_offline(true)
	done = true
	hide()
	
	pass # Replace with function body.

func _on_can_start_scene():
	print(\"WOW\")
	while(!done):
		yield(get_tree().create_timer(1,false),\"timeout\")
		
	AdvancedBackgroundLoader.change_scene_to_preloaded()
"

